<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Controlador</servlet-name>
        <servlet-class>Controlador.Controlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorVisitantes</servlet-name>
        <servlet-class>Controlador.ControladorVisitantes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controlador_Visitantes_Ppl</servlet-name>
        <servlet-class>Controlador.Controlador_Visitantes_Ppl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Validar</servlet-name>
        <servlet-class>Controlador.Validar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controlador_Direcciones</servlet-name>
        <servlet-class>Controlador.Controlador_Direcciones</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controlador_graficas</servlet-name>
        <servlet-class>Controlador.Controlador_graficas</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Controlador</servlet-name>
        <url-pattern>/Controlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorVisitantes</servlet-name>
        <url-pattern>/ControladorVisitantes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controlador_Visitantes_Ppl</servlet-name>
        <url-pattern>/Controlador_Visitantes_Ppl</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Validar</servlet-name>
        <url-pattern>/Validar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controlador_Direcciones</servlet-name>
        <url-pattern>/Controlador_Direcciones</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controlador_graficas</servlet-name>
        <url-pattern>/Controlador_graficas</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
